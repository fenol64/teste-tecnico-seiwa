{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "git": {
    "deploymentEnabled": {
      "main": true,
      "master": true
    }
  },
  "ignoreCommand": "bash vercel-ignore-build.sh"
}